{
    "name": "edusocial-backend",
    "version": "1.0.0",
    "description": "Aplicativo escolar com a finalidade de aumentar a participação dos responsáveis no desenvolvimento dos alunos da educação infantil",
    "main": "index.js",
    "scripts": {
        "start": "tsc && node dist/server/index.js",
        "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --exit-child index.ts",
        "seed": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --exit-child prisma/seed.ts",
        "build": "yarn tsc",
        "lint": "eslint src/**/*.ts",
        "test": "jest",
        "migrate": "npx prisma migrate dev",
        "migrate-deploy": "npx prisma migrate deploy"
    },
    "author": "Felipe Vieira Sobral <xfelipesobral@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^5.21.1",
        "aws-sdk": "^2.1691.0",
        "bcrypt": "^5.1.1",
        "blurhash": "^2.0.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "mime": "3.0.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.15",
        "sharp": "^0.33.5",
        "swagger-ui-express": "^5.0.1",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
        "@types/axios": "^0.14.0",
        "@types/bcrypt": "^5.0.2",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/lodash": "^4.17.9",
        "@types/mime": "3.0.4",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.5.1",
        "@types/nodemailer": "^6.4.16",
        "@types/supertest": "^6.0.2",
        "@types/swagger-ui-express": "^4.1.6",
        "@types/uuid": "^10.0.0",
        "axios": "^1.7.7",
        "jest": "^29.7.0",
        "prettier-plugin-prisma": "^5.0.0",
        "prisma": "^5.21.1",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.5.4"
    }
}